when_list 					::=		  when_element:we
										{: RESULT = new WhenList(); RESULT.add(we); :}
									| when_element:we when_list:wl
										{: 
											wl.add(we); 
											RESULT = wl; 
										:}
									;

when_element 				::=		  WHEN when_options:w OPERPUNTERO statements:s
										{: RESULT = new WhenElement(w,s); :}
									// Error
									| WHEN when_options:w error statements:s {:
											parser.report_error("Se esperaba \"=>\" ","Error: ");
											RESULT = new WhenElement(w,s);
										:}
									//regla anterior sin statements
									| WHEN when_options:w OPERPUNTERO
										{: RESULT = new WhenElement(w); :}
									// Error
									| WHEN when_options:w error {:
											parser.report_error("Se esperaba \"=>\" ","Error: ");
											RESULT = new WhenElement(w);
										:}

									| WHEN OTHERS OPERPUNTERO statements:s
										{: RESULT = new WhenElement(s); :}
									// Error
									| WHEN error OPERPUNTERO statements:s {:
											parser.report_error("Se esperaba \"others\" ","Error: ");
											RESULT = new WhenElement(s);
										:}
									| WHEN OTHERS error statements:s {:
											parser.report_error("Se esperaba \"=>\" ","Error: ");
											RESULT = new WhenElement(s);
										:}
									//regla anterior sin statements
									| WHEN OTHERS OPERPUNTERO
										{: RESULT = new WhenElement(); :}
									// Error
									| WHEN error OPERPUNTERO {:
											parser.report_error("Se esperaba \"others\" ","Error: ");
											RESULT = new WhenElement();
										:}
									| WHEN OTHERS error {:
											parser.report_error("Se esperaba \"=>\" ","Error: ");
											RESULT = new WhenElement();
										:}
									;

when_options  				::= 	  option_when:opt
										{: 
											RESULT = new WhenOptions();
											RESULT.add(opt); 
										:}
									| option_when:opt PIPE when_options:opts
										{: 
											opts.add(opt);
											RESULT = opts;
										:}
									;


option_when					::=		  expression:e 
										{: RESULT = new WhenOption(e); :}
									| range:r
										{: RESULT = new WhenOption(r); :}
									;


