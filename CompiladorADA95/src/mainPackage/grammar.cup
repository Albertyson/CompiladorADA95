										{: RESULT = ws; :}
									| for_statement:fs
										{: RESULT = fs; :}
									| exit_statement:es
										{: RESULT = es; :}
									| loop_statement:ls
										{: RESULT = ls; :}
									| case_statement:cs
										{: RESULT = cs; :}
									| return_statement:rs
										{: RESULT = rs; :}
									;


range						::=		  expression:e1 DOBLEPUNTO expression:e2
										{: RESULT = new Range(e1, e2); :}
									;


variable_ids				::=		  ID:id
										{:
											Identifier identifier = new Identifier(id);
